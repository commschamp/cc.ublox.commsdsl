            <validValue name="SBAS" val="1" />
            <validValue name="Galileo" val="2" />
            <validValue name="BeiDou" val="3" />
            <validValue name="IMES" val="4" />
            <validValue name="QZSS" val="5" />
            <validValue name="GLONASS" val="6" />
